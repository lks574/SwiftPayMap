# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'SwiftPayMap' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
  # UI
  pod 'SnapKit'
  
  # Networking
  pod 'Alamofire'
  pod 'Moya/RxSwift'
  pod 'RxAlamofire'
  
  # Json
  pod 'ObjectMapper'
  pod 'SwiftyJSON'
  
  
  # SNS
  pod 'KakaoOpenSDK', '~> 1.11'
  pod 'FacebookLogin'
  
  # Architecture
  pod 'ReactorKit'
  
  # RX
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxOptional'
  pod 'RxViewController'
  pod 'RxDataSources'
  pod 'RxKeyboard'
  
  # Image
  pod 'Kingfisher'
  
  # Etc
  pod 'Then'
  pod 'ReusableKit'
  
  
end
